{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.coldmessage.io" }],
      "destination": "https://coldmessage.io/:path*",
      "permanent": true
    }
  ]
}

